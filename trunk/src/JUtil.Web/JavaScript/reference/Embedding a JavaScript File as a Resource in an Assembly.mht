             &lt;Scripts&gt;
                &lt;asp:ScriptReference <SPAN style=3D"COLOR: =
blue">Assembly</SPAN>=3D<SPAN style=3D"COLOR: =
#a31515">"SampleControl"</SPAN> Name=3D<SPAN style=3D"COLOR: =
#a31515">"SampleControl.UpdatePanelAnimation.js"</SPAN> /&gt;
             &lt;/Scripts&gt;
            &lt;/asp:ScriptManager&gt;


            &lt;Samples:UpdatePanelAnimationWithClientResource=20
                     ID=3D<SPAN style=3D"COLOR: =
#a31515">"UpdatePanelAnimator1"</SPAN>
                     BorderColor=3D<SPAN style=3D"COLOR: =
